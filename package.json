{
  "name": "react-prop-log",
  "version": "1.1.1",
  "description": "Tiny helper for debugging React props changes",
  "main": "dist/react-prop-log.cjs.js",
  "module": "dist/react-prop-log.esm.js",
  "browser": "dist/react-prop-log.umd.js",
  "scripts": {
    "build": "BABEL_ENV=build rollup -c",
    "dev": "BABEL_ENV=build rollup -c -w",
    "test": "BABEL_ENV=test jest --coverage",
    "test:watch": "BABEL_ENV=test jest --watch",
    "pretest": "npm run build",
    "prepare": "npm run build",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/domain7/react-prop-log.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Domain7",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/domain7/react-prop-log/issues"
  },
  "homepage": "https://github.com/domain7/react-prop-log#readme",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**"
    ],
    "setupFiles": [
      "<rootDir>/test.config.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^25.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^25.0.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "regenerator-runtime": "^0.12.0",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "peerDependencies": {
    "react": "^15 || ^16",
    "react-dom": "^15 || ^16"
  }
}
