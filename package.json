{
  "name": "whats-new",
  "version": "1.0.0",
  "description": "Tiny helper for debugging React props changes",
  "main": "dist/whats-new.cjs.js",
  "module": "dist/whats-new.esm.js",
  "browser": "dist/whats-new.umd.js",
  "scripts": {
    "build": "BABEL_ENV=build rollup -c",
    "dev": "BABEL_ENV=build rollup -c -w",
    "test": "BABEL_ENV=test jest --coverage",
    "test:watch": "BABEL_ENV=test jest --watch",
    "pretest": "npm run build",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/joeflynn/whats-new.git"
  },
  "keywords": [
    "react"
  ],
  "author": "jo flynn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joeflynn/whats-new/issues"
  },
  "homepage": "https://github.com/joeflynn/whats-new#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.2.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.4.1",
    "regenerator-runtime": "^0.12.0",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0"
  }
